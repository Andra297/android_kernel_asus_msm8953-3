/dts-v1/;

#include "msm8953.dtsi"
#include "msm-pmi8950.dtsi"
#include "msm8953-mtp.dtsi"
#include "msm8953-pmi8950.dtsi"
#include "msm8953-camera-sensor-mtp.dtsi"

#include "libra_leo/msm8953-libra_leo.dtsi"
#include "libra_leo/msm-pmi8950-libra_leo.dtsi"
#include "libra_leo/msm8953-mtp-libra_leo.dtsi"
#include "libra_leo/msm8953-pmi8950-libra_leo.dtsi"
#include "libra_leo/msm8953-camera-sensor-mtp-libra_leo.dtsi"

/ {
	model = "Qualcomm Technologies, Inc. MSM8953 MTP LIBRA SR1 ";
	compatible = "qcom,msm8953-mtp", "qcom,msm8953", "qcom,mtp";
	qcom,board-id= <21 0>;
};

/{
	mtp_batterydata: qcom,battery-data {
		qcom,batt-id-range-pct = <15>;
		#include "batterydata-itech-3000mah.dtsi"
		#include "batterydata-ascent-3450mAh.dtsi"
	};
};

&pmi8950_fg {
	qcom,battery-data = <&mtp_batterydata>;
};

&pmi8950_charger {
	qcom,battery-data = <&mtp_batterydata>;
	/delete-property/ qcom,chg-led-sw-controls;
	/delete-property/ qcom,chg-led-support;
	qcom,external-typec;
	qcom,typec-psy-name = "typec";
};
